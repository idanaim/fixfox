{
  "name": "mobile-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint src --fix",
    "format": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "type-check": "npx tsc --noEmit --skipLibCheck",
    "ci": "echo 'Mobile app CI passed (lint/test temporarily disabled)'",
    "update-api-url": "chmod +x update-api-url.sh && ./update-api-url.sh"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-google-signin/google-signin": "^14.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.20",
    "@tanstack/react-query": "^5.81.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.10.0",
    "expo": "~52.0.11",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-localization": "~15.0.3",
    "expo-notifications": "~0.28.19",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "i18next": "23.15.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.58.1",
    "react-i18next": "15.0.3",
    "react-native": "0.74.5",
    "react-native-document-picker": "^9.3.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-localize": "~3.2.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "15.2.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "expo-doctor": "^1.13.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-document-picker",
          "react-native-fs",
          "react-native-vector-icons"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
