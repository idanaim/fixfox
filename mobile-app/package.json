{
  "name": "mobile-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "export:web": "expo export --platform web",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "submit:production": "eas submit --profile production",
    "ci": "npm run lint:check && npm run format:check && npm run type-check && npx expo-doctor"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-google-signin/google-signin": "^14.0.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.20",
    "@tanstack/react-query": "^5.81.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.10.0",
    "expo": "~52.0.11",
    "expo-document-picker": "~13.0.3",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.1",
    "i18next": "^23.15.1",
    "react": "18.3.1",
    "react-hook-form": "^7.58.1",
    "react-i18next": "^15.0.3",
    "react-native": "0.76.9",
    "react-native-document-picker": "^9.3.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-image-picker": "^8.2.1",
    "react-native-localize": "~3.2.1",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "15.8.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "typescript": "~5.3.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["react-native-document-picker", "react-native-fs", "react-native-vector-icons"],
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
